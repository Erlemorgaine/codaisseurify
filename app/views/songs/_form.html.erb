<%= form_for [@artist, @artist.songs.new] do |f| %>

  <ul>
    <% @artist.errors.full_messages.each do |msg| %>
      <li><%= msg %></li>
    <% end %>
  </ul>

  <div class="form-group">
    <label>Name</label>
    <%= f.text_field :name, placeholder: "Name", class: "form-control" %>
  </div>

  <div class="form-group">
    <label>Year</label>
    <%= f.number_field :name, placeholder: "Name", class: "form-control" %>
  </div>

  <!-- <div class="form-group">
    <label>Year</label>
    <%#= f.select_year() :year id: "year", prompt: "Select year", class: "form-control" %>
  </div> -->

  <div class="form-group">
    <label>Album</label>
    <%= f.text_field :album, placeholder: "Album", class: "form-control" %>
  </div>

  <div class="actions">
    <%= f.submit "Save", class: "btn btn-primary" %>
  </div>
<% end %>
